= render 'products/shared/header'

%nav.breadCrumbs
  - breadcrumb :mypage
  = render "layouts/breadcrumbs"

%main.container
  .container__inner
    = render 'side-bar'

    .container__inner__content
      %section.container__inner__content
        - if params[:likes].present?
          .mypageProductLists#likesval
            = render partial: 'like-products', locals: { likes: @likes }
        - else
          .mypageProductLists#likesval
            = render 'mypage_products-lists'

= render 'products/shared/app_banner'

= render 'products/shared/footer'

= render 'products/shared/purchase_btn'
